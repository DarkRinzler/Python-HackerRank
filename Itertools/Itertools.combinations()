from itertools import combinations
from typing import List

def get_input() -> list:
    elements = input().split()
    return sorted(elements[0]), int(elements[1])
    
def generate_combinations(iterable: List[str] , length: int) -> List[str]:
    results = []
    for i in range(1, length + 1):
        results.extend(''.join(perm) for perm in combinations(iterable, i))
    return results
    
def main() -> None:
    iterable, length = get_input()
    results = generate_combinations(iterable, length)
    print('\n'.join(results))
    
if __name__ == "__main__":
    main()
    
